{
    "@context":
    {
        "@vocab": "http://schema.org/"
    },
    "@type": "Dataset",
    {#- todo: correct way of describing the Identifier, 
    taking into account 'DatasetIdentifier' and 'DataLandingPageURL' 
    (which can hold multiple values separated by '|') 
    from google sheets) #}
    "@id": "{{}}",
    {# todo: add correct distribution - will not always be available, see identifier issue above
    "distribution": {
      "@type": "DataDownload",
        "contentUrl": "http://marineinfo.org/id/dataset/{{_.datasetrec.DasID}}.jsonld" ,
        "encodingFormat": "https://www.iana.org/assignments/media-types/application/ld+json"
    }
    #}
    {#
    {%- "includedInDataCatalog" --> for datasetdescr in sets['datasetdescription'] %}
      {%- if datasetdescr.DasID == _.datasetrec.DasID %}
    "includedInDataCatalog":  {
        {%- if datasetdescr.dataCatalogID %}
        "@id": "{{datasetdescr.dataCatalogID}}",
        {%- endif %}
        {%- if datasetdescr.dataCatalogUrl %}
        "url": "{{datasetdescr.dataCatalogUrl}}",
        {%- endif %}
        "@type": "DataCatalog"
      } {%- if not loop.last %}, {%- endif %}
      {%- endif %}
    {%- endfor %}
    #}
    {#- "measurementTechnique" --> in dataset description on oceaninfohub, but not in json record of dataset - information could be included in MBO spreadsheets #}
    {#
    {%- for datasetdescr in sets['datasetdescription'] %}
      {%- if datasetdescr.DasID == _.datasetrec.DasID %}
    {%- if datasetdescr.measurementTechnique %}
    "measurementTechnique": "{{datasetdescr.measurementTechnique}}" ,
    {%- endif %}
      {%- endif %}
    {%- endfor %}
    #}
    {#- meastypes#}
    {%- if _.meastypes %}
    "variableMeasured": [
    {%- for meastype in _.meastypes %}
        {%- if meastype.Parameter or meastype.Description %}
         {
            {%- if meastype.Parameter %}
            "name":"{{meastype.Parameter}}",
            {%- else %}
            "name":"",
            {%- endif %}
             {%- if meastype.Description and meastype.Description.startswith('http') %}
            "identifier":"{{ meastype.Description}}",
             {%- elif meastype.Description %}
            "description":"{{ meastype.Description}}",
             {%- endif %}
            "@type":"PropertyValue"
        } {%- if not loop.last %}, {%- endif %}
        {%- endif %}
    {%- endfor %}
    ] , 
    {%- endif %}
    {#- "about"#}
    {%- if _.datasetrec.EngAbstract %}
    "abstract": "{{_.datasetrec.EngAbstract | replace('\r\n',' ') | replace('\r\t',' ') | replace('\n',' ') | replace('\t',' ') }}",
    {%- endif %}
    {#- "accountablePerson"#}
    {#- "acquireLicensePage"#}
    {#- "archivedAt"#}
    {#- "associatedMedia"#}
    {#- "audience"#}
    {#- "author"#}
    {%- if _.datasetrec.Citation %}
    "citation": "{{_.datasetrec.Citation | replace('\r\n',' ') | replace('\r\t',' ') | replace('\n',' ') | replace('\t',' ') }}",
    {%- endif %}
    {%- if _.datasetrec.AccConstrDescription %}
    "conditionsOfAccess": "{{_.datasetrec.AccConstrDescription | safe }}",
    {%- endif %}
    {#- "contentReferenceTime"#}
    {#- "contributor"#}
    {#- "copyrightHolder"#}
    {#- "copyrightNotice"#}
    {#- "copyrightYear"#}
    {#- "correction"#}
    {#- "countryOfOrigin"#}
    {%- if _.datasetrec.Progress %}
    "creativeWorkStatus": "{{_.datasetrec.Progress}}",
    {%- endif %}
    {#- "creditText"#}
    {%- if _.datasetrec.CDate %}
    "datePublished": "{{_.datasetrec.CDate}}",
    {%- endif %}
    {%- if _.datasetrec.UDate %}
    "dateModified": "{{_.datasetrec.UDate}}",
    {%- endif %}
    {#- "dateCreated"#}
    {#- "discussionUrl"#}
    {#- "editor"#}
    {#- "educationalUse"#}
    {#- "encoding"#}
    {#- "encodingFormat"#}
    {#- "funder"#}
    {#- "funding"#}
    {%- if _.children %}
    "hasPart": [ 
      {%- for child in _.children %}
      "http://marineinfo.org/id/dataset/{{child.DasID}}"
      {%- if not loop.last %}, {%- endif %}
      {%- endfor %}
    ] , 
    {%- endif %}
    {#- "inLanguage"#}
    {#- "isAccessibleForFree"#}
    {#- "isBasedOn"#}
    {%- if _.parents %}
    "isPartOf": [
      {%- for parent in _.parents %}
      "http://marineinfo.org/id/dataset/{{parent.DasID}}"
      {%- if not loop.last %}, {%- endif %}
      {%- endfor %}
    ] , 
    {%- endif %}
    {%- if _.keywords %}
    "keywords": [
        {%- for keyw in _.keywords %}
        {
            {%- if keyw.ThesaurusTerm %}
            "name":"{{keyw.ThesaurusTerm}}",
            {%- endif %}
            {%- if keyw.ThesType %}
            "inDefinedTermSet":"{{keyw.ThesType}}",
            {%- endif %}
            {%- if keyw.ThesTypID %}
            "termCode":"{{keyw.ThesTypID}}",
            {%- endif %}
            "@type":"DefinedTerm"
        } {%- if not loop.last %}, {%- endif %}
        {%- endfor %}
    ],
    {%- endif %}
    {%- if _.datasetrec.License %}
    "license": "{{_.datasetrec.License}}",
    {%- endif %}
    {#- "locationCreated"#}
    {#- "mainEntity"#}
    {#- "maintainer"#}
    {#- "position"#}
    {#- "provider" --> in dataset description on oceaninfo hub, however not included in json record of dataset - information that could be recorded in MBO spreadsheets #}
    {#
    {%- for datasetdescr in sets['datasetdescription'] %}
      {%- if datasetdescr.DasID == _.datasetrec.DasID and datasetdescr.DataProviderAgent %}
    "provider" : 
      {%- for provider in _.DataProviderAgent.split("|") %}
        {% for agent in sets["agents"]  %}
          {%- if provider == agent.AgentKey %}
      {
        "legalName": "{{agent.legalName}}" ,
        {%- if agent.Name %}
        "name": "{{agent.Name}}",
        {%- endif %}
        {%- if agent.URL %}
        "url" : "{{agent.URL}}" ,
        {%- endif %} 
        "@type": "https://schema.org/{{agent.Type}}"
      } {%- if not loop.last %}, {%- endif %}
          {%- endif %}
        {%- endfor %}
      {%- endfor %}
      {%- endif %}
    {%- endfor %}
    #}
    {#- "publication"#}
    {%- if _.ownerships %}
    {%- set creators = [] %}
    {%- set contributors = [] %}
    {%- set publishers = [] %}
    {%- for ownership in _.ownerships %}
      {%- if ownership.RoleID == 61 %}
        {%- set _ = creators.append(ownership) %}
      {%- elif ownership.RoleID == 24 %}
        {%- set _ = publishers.append(ownership) %}
      {%- else %}
        {%- set _ = contributors.append(ownership) %}
      {%- endif %}
    {%- endfor %}
    {%- endif %}
    "creator" : [
    {%- for ownership in creators %}
      {%- if ownership.PersID %}
        "http://marineinfo.org/id/person/{{ownership.PersID}}"
      {%- elif ownership.InsID %}
        "http://marineinfo.org/id/institute/{{ownership.InsID}}"
      {%- endif %}
      {%- if not loop.last %}, {%- endif %}
    {%- endfor %}
    ] ,
    "contributor": [
    {%- for ownership in contributors %}
      {%- if ownership.PersID %}
        "http://marineinfo.org/id/person/{{ownership.PersID}}"
      {%- elif ownership.InsID %}
        "http://marineinfo.org/id/institute/{{ownership.InsID}}"
      {%- endif %}
      {%- if not loop.last %}, {%- endif %}
    {%- endfor %}
    ] ,
    "publisher" : [
    {%- for ownership in publishers %}
      {%- if ownership.PersID %}
        "http://marineinfo.org/id/person/{{ownership.PersID}}"
      {%- elif ownership.InsID %}
        "http://marineinfo.org/id/institute/{{ownership.InsID}}"
      {%- endif %}
      {%- if not loop.last %}, {%- endif %}
    {%- endfor %}
    ] , 
    {#- "publishingPrinciples" --> in EOV description on oceaninfohub, not in json record - could be inlcuded in MBO spreadsheets #}
    {#
    {%- for datasetdescr in sets['datasetdescription'] %}
      {%- if datasetdescr.DasID == _.datasetrec.DasID %}
    {%- if datasetdescr.pubPrincUrl or datasetdescr.pubPrincName or datasetdescr.pubPrincDescr %}
    "publishingPrinciples": {
      {
            {%- if datasetdescr.pubPrincUrl %}
            "url": "{{datasetdescr.pubPrincUrl}}",
            {%- endif %}
            {%- if datasetdescr.pubPrincName %}
            "name": "{{datasetdescr.pubPrincName}}",
            {%- endif %}
            {%- if datasetdescr.pubPrincDescr %}
            "description": "{{datasetdescr.pubPrincDescr}}" ,
            {%- endif %}
            "@type": "CreativeWork"
        },
    } ,
    {%- endif %}
      {%- endif %}
    {%- endfor %}
    #}
    {#- "review"#}
    {#- "sdDatePublished"#}
    {#- "size"#}
    {#- "spatial"#} 
    {%- if _.geographical %}
    "spatialCoverage":
    [
        {%- for geot in _.geographical %}
        {
            "geoOverlaps" : {
            {%- if geot.OrigGeoTerm and geot.OrigGeoTerm != "EurOBIS calculated BBOX" %}
            "name": "{{geot.OrigGeoTerm}}" , 
            {%- endif %}
            {%- if geot.GeoObjectID %}
            "identifier": "http://www.marineregions.org/mrgid/{{geot.GeoObjectID}}"
            {%- endif %}
            },
            {%- if geot.X and geot.Y and geot.MaxX and geot.MaxY %}
            "geo": {
                "box" : "{{geot.X}} {{geot.Y}} {{geot.MaxX}} {{geot.MaxY}}",
                "@type": "GeoShape"
            },
            {%- endif %}
            {%- if geot.GeoDatum %}
              {%- if geot.OrigCoordMinX and geot.OrigCoordMinY %}
            "geo": {
                "latitude": {{geot.OrigCoordMinY}},
                "longitude": {{geot.OrigCoordMinX}},
                "@type": "GeoCoordinates"
            },
            {%- endif %}
            "additionalProperty": {
              "@type": "PropertyValue",
              "propertyID":"http://dbpedia.org/resource/Spatial_reference_system",
              {%- if geot.GeoDatum == "WGS84" %}
              "value": "http://www.w3.org/2003/01/geo/wgs84_pos#lat_long"
              {%- endif %}
              {%- if geot.GeoDatum == "CRS84" %}
              "value": "http://www.opengis.net/def/crs/OGC/1.3/CRS84"
              {%- endif %}
              {%- if geot.GeoDatum == "EPSG:26911" %}
              "value": "https://spatialreference.org/ref/epsg/nad83-utm-zone-11n/"
              {%- endif %}
              {%- if geot.GeoDatum == "EPSG:3413" %}
              "value": "https://spatialreference.org/ref/epsg/wgs-84-nsidc-sea-ice-polar-stereographic-north/"
              {%- endif %}
            },
            {%- if geot.OrigCoordMinX and geot.OrigCoordMinY and geot.OrigCoordMaxX and geot.OrigCoordMaxY %}
            "geo": {
                "box" : "{{geot.OrigCoordMinX}} {{geot.OrigCoordMinY}} {{geot.OrigCoordMaxX}} {{geot.OrigCoordMaxY}}",
                "@type": "GeoShape"
            },
            {%- endif %}
            {%- endif %}
            "@type": "Place"
        } {%- if not loop.last %}, {%- endif %}
        {%- endfor %}
    ],
    {%- endif %}
    {#- "temporal"#} 
    {%- if _.temporal %}
    "temporalCoverage":
    [
        {%- for temp in _.temporal %}
        {%- if temp.StartDate and temp.EndDate and temp.StartDate == temp.EndDate %}
        "{{temp.StartDate}}"
        {%- elif temp.StartDate and temp.EndDate %}
        "{{temp.StartDate}}/{{temp.EndDate}}"
        {%- elif temp.StartDate and temp.Progress == "In Progress" %}
        "{{temp.StartDate}}/.."
        {%- endif %}
        {%- if not loop.last %}, {%- endif %}
        {%- endfor %}
    ],
    {%- endif %}
    {%- if _.taxterms %}
    "about" : [ {#note: "about" in EOV description on oceaninfohub, but for same content as used with 'subjectOf' in dataset description on oceaninfohub - so leaving in as is #}
        {%- for tax in _.taxterms %}
        {
          "taxonRank" : {
            "name": "{{tax.TaxTerm}}",
            "identifier": "https://www.marinespecies.org/aphia.php?p=taxdetails&id={{tax.AphiaID}}"
          },          
          "@type" : "Taxon"
        } {%- if not loop.last %}, {%- endif %}
        {%- endfor %}
    ],
    {%- endif %}
    {%- if _.datasetrec.VersionName %}
    "version": "{{_.datasetrec.VersionName}}",
    {%- endif %}
    {#- "additionalType"#}
    {%- if _.datasetrec.Acronym %}
    "alternateName": "{{_.datasetrec.Acronym}}",
    {%- endif %}
    {%- if _.datasetrec.EngDescr %}
    "description": "{{_.datasetrec.EngDescr | replace('\r\n',' ') | replace('\r\t',' ') | replace('\n',' ') | replace('\t',' ') }}",
    {%- endif %}
    {%- if _.datasetrec.OrigDescr %}
    "disambiguatingDescription": "{{_.datasetrec.OrigDescr}}",
    {%- endif %}
    "identifier": "{{_.datasetrec.DasID}}",
    {#- "mainEntityOfPage"#}
    {%- if _.datasetrec.StandardTitle %}
    "name": "{{_.datasetrec.StandardTitle}}",
    {%- endif %}
    "sameAs": "http://marineinfo.org/id/dataset/{{_.datasetrec.DasID}}",
    {#- "subjectOf" --> in dataset description on oceaninfohub, but not included in json record of dataset - information that could possibly be included in the MBO spreadsheet (if such info where applicable) #}
    {#
    {%- for datasetdescr in sets['datasetdescription'] %}
      {%- if datasetdescr.DasID == _.datasetrec.DasID %}
    "subjectOf": {
        "description": "{{datasetdescr.activityDescription}}",
        "name": "{{datasetdescr.activityName}}",
        "potentialAction": [
            {%- if datasetdescr.activityAction %}
            {%- for action in datasetdescr.activityAction.split("|") %}
            {
                {%- if action.name %}
                "name": "{{action.name}}",
                {%- endif %}
                {%- if action.agentID %}
                "agent" "{{action.agentID}}",
                {%- endif %}
                {%- if action.startTime %}
                "startTime": "{{action.startTime}}",
                {%- endif %}
                {%- if action.endTime %}
                "endTime": "{{action.endTime}}",
                {%- endif %}
                {%- if action.instrumentName or action.instrumentUrl or action.instrumentDescription %}
                "instrument": {
                    {%- if action.instrumentName %}
                    "name": "{{action.instrumentName}}",
                    {%- endif %}
                    {%- if action.instrumentUrl %}
                    "url": "{{action.instrumentUrl}}",
                    {%- endif %}
                    {%- if action.instrumentDescription %}
                    "description": "{{action.instrumentDescription}}",
                    {%- endif %}
                    "@type": "Thing"
                },
                {%- endif %}
                "@type": "Action"
            } 
            {%- endfor %}
            {%- endif %}
        ],
      "@type": "Event"
    } ,
      {%- endif %}
    {%- endfor %}
    #}
    "url": "http://marineinfo.org/id/dataset/{{_.datasetrec.DasID}}"
}