    "datasetrec": {
        "DasID": 4554,
        "Acronym": null,
        "StandardTitle": "National Inventory of the Natural Heritage: Data from the air monitoring campaigns of marine megafauna (SAMM) in the French metropolitan area",
        "OrigTitle": "Inventaire National du Patrimoine Naturel: Donn\u00e9es des campagnes de Suivi A\u00e9rien de la M\u00e9gafaune Marine (SAMM) de France m\u00e9tropolitaine",
        "OrigTitleLangID": 22,
        "OrigTitleLangCode": "fr",
        "OrigTitleLang": "French",
        "OrigTitleLangNL": "Frans",
        "VersionName": null,
        "ContactEmail": null,
