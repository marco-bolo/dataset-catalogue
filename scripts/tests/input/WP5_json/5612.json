    "datasetrec": {
        "DasID": 5612,
        "Acronym": null,
        "StandardTitle": "Trawl survey data from the Jabuka Pit area (central-eastern Adriatic Sea, Mediterranean) collected between 1956 and 1971",
        "OrigTitle": null,
        "OrigTitleLangID": null,
